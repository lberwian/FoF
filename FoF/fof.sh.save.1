#!/bin/bash
myprog=~/fof/FoF/FoF
echo "Por favor insira o número de execuções: "
read -p "Número de execuções para o teste de desempenho " N 
read -p "Arquivo de entrada " arc 
read -p "Raio " R
read -p "Número de elementos " E
read -p "Para escrever o arquivo com resultados digite S/s, ou qualquer coisa para não: " read


a=0

while [ $a -ne $N ];
do
    	start=`date +%s`
	$myprog "$arc" "$E" "$R" "$read"
	end=`date +%s`
	runtime=$((end-start))
	echo "Tempo $runtime"
	a=$(($a+1))
done

